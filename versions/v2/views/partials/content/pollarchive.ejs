</div>

<% if (Object.keys(groupedPolls).length > 0) { %>
	<% Object.keys(groupedPolls).forEach(function(monthYear) { %>
	<div class="brspacer">
		<div class="title"><img class="pre" src="/images/pres/pre3.gif"> <%= monthYear %></div>
	</div>
	<div class="color2">
		<% groupedPolls[monthYear].forEach(function(poll) { %>
		<div class="archiverow">
			<img class="pre" src="/images/pres/pre.gif"> <a href="/<%= poll.slug %>/"><%= poll.question %></a> <span>on <%= poll.formattedDate %></span><br>
		</div>
		<% }); %>
	</div>
	<div class="dots"></div>
	<% }); %>
<% } else { %>
<div class="brspacer">
	No polls found.
</div>
<% } %>

