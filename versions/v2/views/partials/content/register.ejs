<form action="" name="register" class="formdata" method="post">
	<table class="max">
		<tbody><tr>
			<td class="title" colspan="2"><img class="pre" src="/images/pres/pre3.gif"> Account Information (required)</td>
		</tr>
		<tr class="color2">
			<td class="info" colspan="2">Your username cannot be changed after registration.</td>
		</tr>
		<tr class="color1">
			<td width="25%">
				<label for="username1">Username:</label>
			</td>
			<td width="75%">
				<input type="text" name="username1" id="username1" maxlength="16" value="" size="35">
                <% if (typeof error !=='undefined' && error==='username' ) { %>
                    <div class="error">
                        <%= errorMessage %>
                    </div>
                <% } %>
			</td>
		</tr>
		<tr class="color2">
			<td class="info" colspan="2">Fill in your desired password. This password needs to be at least 3 characters long and is not case sensitive. The max. number of characters is 16.</td>
		</tr>
		<tr class="color1">
			<td>
				<label for="password1">Password:</label>
			</td>
			<td>
				<input type="password" name="password1" id="password1" value="">
                <% if (typeof error !=='undefined' && error==='password' ) { %>
                    <div class="error">
                        <%= errorMessage %>
                    </div>
                <% } %>
			</td>
		</tr>
		<tr class="color2">
			<td class="info" colspan="2">Please fill in your password again to make sure you didn't type any mistakes.</td>
		</tr>
		<tr class="color1">
			<td>
				<label for="password2">Confirm Password:</label>
			</td>
			<td>
				<input type="password" name="password2" id="password2" value="">
                <% if (typeof error !=='undefined' && error==='password2' ) { %>
                    <div class="error">
                        <%= errorMessage %>
                    </div>
                <% } %>
			</td>
		</tr>
		<tr class="color2">
			<td class="info" colspan="2">Fill in a valid e-mail address.</td>
		</tr>
		<tr class="color1">
			<td>
				<label for="email">E-mail:</label>
			</td>
			<td>
				<input type="text" name="email" id="email" value="" size="35">
                <% if (typeof error !=='undefined' && error==='email' ) { %>
                    <div class="error">
                        <%= errorMessage %>
                    </div>
                <% } %>
			</td>
		</tr>
		<tr class="color2">
			<td class="info" colspan="2">By checking this box your e-mail address will be hidden from other members.</td>
		</tr>
		<tr class="color1">
			<td>
				<label for="emailhide">Hide e-mail</label>
			</td>
			<td>
				<input type="checkbox" class="nb" name="emailhide" id="emailhide" value="1">
			</td>
		</tr>
		<tr><td class="dots" colspan="2"></td></tr>
	</tbody></table>
