<% if (typeof success !=='undefined' && success) { %>
    <div class="succesfull" style="text-align: center;">
        <%= success %>
    </div>
<% } %>
<% if (typeof error !=='undefined' && error) { %>
    <div class="error" style="text-align: center;">
        <%= error %>
    </div>
<% } %>

<form method="post" class="formdata">
	<table class="max">
		<tbody><tr>
			<td class="title" colspan="2"><img class="pre" src="/images/pres/pre3.gif"> Reset Account</td>
		</tr>
		<tr class="color2">
			<td class="info" colspan="2">This will reset your account statistics to their default values:</td>
		</tr>
		<tr class="color1">
			<td width="25%"><b>Wins:</b></td>
			<td width="75%">0</td>
		</tr>
		<tr class="color1">
			<td><b>Losses:</b></td>
			<td>0</td>
		</tr>
		<tr class="color1">
			<td><b>Streak:</b></td>
			<td>0</td>
		</tr>
		<tr class="color1">
			<td><b>Level:</b></td>
			<td>1</td>
		</tr>
		<tr class="color1">
			<td><b>Ladder Position:</b></td>
			<td>None</td>
		</tr>
		<tr class="color1">
			<td><b>Rank:</b></td>
			<td>Academy Student</td>
		</tr>
		<tr class="color2">
			<td class="info" colspan="2"><b>Warning:</b> This action cannot be undone!</td>
		</tr>
		<tr><td class="dots" colspan="2"></td></tr>
	</tbody></table>
	<div class="centerp">
		<input type="submit" class="button" name="resetaccount" value="Reset Account">
	</div>
</form>
