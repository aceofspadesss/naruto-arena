<% if (Object.keys(groupedNews).length > 0) { %>
	<% for (const groupKey in groupedNews) { %>
	<div class="brspacer">
		<div class="title"><img class="pre" src="/images/pres/pre3.gif"> <%= groupKey %></div>
	</div>
	<div class="color2">
		<% groupedNews[groupKey].forEach(function(news) { %>
		<div class="archiverow">
			<img class="pre" src="/images/pres/pre.gif"> <a href="/<%= news.slug %>/"><%= news.title %></a> <span>on <%= news.formattedDate %> </span><br>
		</div>
		<% }); %>
	</div>
	<div class="dots"></div>
	<% } %>
<% } else { %>
<div class="brspacer">
	No news found.
</div>
<% } %>

