        <br><br>


        <center>
        </center>

        <% if (Object.keys(groupedNews).length> 0) { %>
            <% for (const groupKey in groupedNews) { %>
                <h5>
                    <%= groupKey %>
                </h5><br>
                <% groupedNews[groupKey].forEach(news=> { %>
                    <img class='pre' src='/images/layout/pre.gif'> <a href="/<%= news.slug %>/"><u><%= news.title %></u></a>
                    <h3>
                        <%= news.formattedDate %>
                    </h3> <br>
                    <% }); %>
                        <br>
                        <% } %>
                            <% } else { %>
                                <p>No news found.</p>
                                <% } %>
                                    <div class="dots"></div>
                                    <div class="nothing">
