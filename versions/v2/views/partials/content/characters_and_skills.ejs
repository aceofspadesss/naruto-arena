		<img class="pre" src="/images/pres/pre3.gif"> <h2><%= character.name %></h2>
		<div class="dots"></div>
		<div class="chardescr">
			<img src="/game/images/characters/<%= character.id %>/avatarselection.jpg" class="border" align="left"><%= character.description %>
		</div>
		<div class="dots"></div>
		<div class="right">
			<a href="/<%= slug %>/" class="subjlink">More about <%= character.name %></a>
		</div>
	</div>
    <% }); %>
	
