<script type="text/javascript" language="javascript">
	function gosearch(){
		var searchvalue = document.searchform.searchquery.value;
		searchvalue = searchvalue.replace(" ","+");
		searchvalue = searchvalue.toLowerCase();
		if (searchvalue != ""){
			var completeurl = "/search/" + searchvalue + "/";
		}else{
			var completeurl = "/search/";
		}
		window.location = completeurl;
	}
</script>


