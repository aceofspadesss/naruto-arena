<div class="cbrspacer">
  <h4 class="block">Top 10 Winstreak</h4>
</div>
<% if (topStreak && topStreak.length > 0) { %>
  <% topStreak.forEach((player, index) => { %>
    <ul class="ladder">
      <li>
        <span><%= index + 1 %>.</span>
        <div>
          <a href="/profile/<%= player.username %>"><%= player.username %></a> <b>+<%= player.streak %></b> streak
        </div>
      </li>
    </ul>
  <% }); %>
<% } else { %>
  <li>
    <div>No players with win streaks yet.</div>
  </li>
<% } %>